{
  "name": "realm-ts-class-decorators",
  "version": "0.3.2",
  "description": "Typescript support and class utils for realm-js",
  "homepage": "https://github.com/aklinker1/realm-ts-class-decorators",
  "keywords": [
    "realm",
    "realm-js",
    "react-native",
    "typescript",
    "class",
    "decorators"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "engines": {
    "node": "10"
  },
  "files": [
    "lib/**/*",
    "README.md",
    "coverage/badge-*.svg"
  ],
  "repository": "github.com/aklinker1/realm-ts-class-decorators",
  "author": "Aaron Klinker <aaronklinker1@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc -p .",
    "lint": "eslint -c .eslintrc.js src/**/*.ts",
    "lint:fix": "eslint -c .eslintrc.js --fix src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --config jest.config.js --watch",
    "test:coverage": "jest --config jest.config.js --coverage && jest-coverage-badges"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "eslint": "^6.7.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-only-warn": "^1.0.1",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "jest-coverage-badges": "^1.1.2",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "realm": "^3.4.2"
  }
}
