{
  "name": "realm-ts-class-decorators",
  "version": "1.0.0",
  "description": "Typescript support and class utils for realm-js",
  "keywords": [
    "realm",
    "realm-js",
    "typescript",
    "class",
    "decorators"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "repository": "github.com/aklinker1/realm-ts-class-decorators",
  "author": "Aaron Klinker <aaronklinker1@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc -p .",
    "lint": "eslint -c .eslintrc.js src/**/*.ts",
    "lint:fix": "eslint -c .eslintrc.js --fix src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest --config jest.config.js"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "eslint": "^6.7.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.2"
  }
}
